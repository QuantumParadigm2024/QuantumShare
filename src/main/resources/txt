<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- Root logger configuration (optional) -->
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

    <!-- Console appender -->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} - %msg%n</pattern>
        </encoder>
    </appender>

    <!-- Set specific logger for CSVReaderUtil to WARN or ERROR -->
    <logger name="com.qp.quantum_share.helper.CSVReaderUtil" level="WARN" />
</configuration>
